            polygon(points="0,0 0,160 900.8,161 900,157").triangle_fill_lightwhite
            polygon(points="1800,0 1800,160 900.8,161 900,157").triangle_fill_lightwhite
        
        //- инпуты для блога
        .siteBody__content
